import { OnInit } from '@angular/core';
import { PositionType, SpinnerType } from './utils/types';
import { NgxLoadingXConfig } from './utils/NgxLoadingXConfig.interface';
import { DomSanitizer, SafeResourceUrl } from '@angular/platform-browser';
import { NgxLoadingXService } from './ngx-loading-x.service';
export declare class NgxLoadingXComponent implements OnInit {
    private domSanitizer;
    private ngxLoadingXService;
    show: boolean;
    bgBlur: number;
    bgColor: string;
    bgOpacity: number;
    bgLogoUrl: string;
    bgLogoUrlPosition: PositionType;
    bgLogoUrlSize: number;
    spinnerType: SpinnerType;
    spinnerSize: number;
    spinnerColor: string;
    spinnerPosition: PositionType;
    defaultConfig: NgxLoadingXConfig;
    spinnerDivs: Number[];
    spinnerClass: string;
    trustedLogoUrl: SafeResourceUrl;
    spinnerTop: any;
    logoTop: any;
    constructor(domSanitizer: DomSanitizer, ngxLoadingXService: NgxLoadingXService);
    private initializeSpinners;
    private determineCenterPositions;
    ngOnInit(): void;
}
